{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "XDebug",
      "type": "php",
      "request": "launch",
      "port": 9003,
      "windows":{
        "localSourceRoot": "${workspaceRoot}",
        "serverSourceRoot": "${env:WSL_XFORGE_PATH}"
      }
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach to Chrome",
      "port": 9222,
      "webRoot": "${workspaceRoot}"
    }
  ]
}
